    <skos:inScheme rdf:resource="http://energy.referencedata.eu/StandardPriceCategoryTypeList"/>
    <skos:topConceptOf rdf:resource="http://energy.referencedata.eu/StandardPriceCategoryTypeList"/>
    <dcterms:identifier>A08</dcterms:identifier>
    <skos:prefLabel>Cross-border marginal price</skos:prefLabel>
    <skos:definition xml:lang="en">The price determined in accordance with article 3 of the methodology for pricing balancing energy.</skos:definition>
  </skos:Concept>
