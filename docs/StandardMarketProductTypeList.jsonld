            },
            "@dcterms:identifier": "A09",
            "@skos:prefLabel": "Strategic reserve resource capacity mechanism",
            "@skos:definition": {
                "@language": "en",
                "@value": "This is a market resource capacity mechanism that is kept outside of the electricity market and only used if the market participants do not offer enough generation to meet short-term demand."
            }
        },
        {
            "@id": "http://energy.referencedata.eu/StandardMarketProductTypeList/A08",
            "@type": "@skos:Concept",
