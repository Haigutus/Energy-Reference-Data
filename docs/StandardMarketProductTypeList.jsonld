            },
            "@dcterms:identifier": "A01",
            "@skos:prefLabel": "Standard balancing product",
            "@skos:definition": {
                "@language": "en",
                "@value": "A harmonised balancing product defined by all TSOs for the exchange of balancing services."
            }
        },
        {
