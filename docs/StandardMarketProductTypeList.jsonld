            "@dcterms:identifier": "A04",
            "@skos:prefLabel": "Local balancing product",
            "@skos:definition": {
                "@language": "en",
                "@value": "A balancing product that is neither standard nor specific. This type may be applicable only in the interim period until standard and specific products have been defined within the given scheduling area."
            }
        },
