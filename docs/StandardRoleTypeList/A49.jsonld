    "@dcterms:identifier": "A49",
    "@skos:prefLabel": "Transmission System Operator (TSO)",
    "@skos:definition": {
        "@language": "en",
        "@value": "The Transmission System Operator (TSO) is responsible for the transport of electricity on the extra high-voltage and high-voltage interconnected system. This is a market participant and not a role in Harmonised Role Model."
    },
