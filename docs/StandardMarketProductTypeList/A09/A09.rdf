  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about="http://energy.referencedata.eu/StandardMarketProductTypeList/A09">
    <skos:inScheme rdf:resource="http://energy.referencedata.eu/StandardMarketProductTypeList"/>
    <skos:topConceptOf rdf:resource="http://energy.referencedata.eu/StandardMarketProductTypeList"/>
    <dcterms:identifier>A09</dcterms:identifier>
    <skos:prefLabel>Strategic reserve resource capacity mechanism</skos:prefLabel>
    <skos:definition xml:lang="en">This is a market resource capacity mechanism that is kept outside of the electricity market and only used if the market participants do not offer enough generation to meet short-term demand.</skos:definition>
  </skos:Concept>
