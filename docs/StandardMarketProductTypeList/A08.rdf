  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about="http://energy.referencedata.eu/StandardMarketProductTypeList/A08">
    <skos:inScheme rdf:resource="http://energy.referencedata.eu/StandardMarketProductTypeList"/>
    <skos:topConceptOf rdf:resource="http://energy.referencedata.eu/StandardMarketProductTypeList"/>
    <dcterms:identifier>A08</dcterms:identifier>
    <skos:prefLabel>Market wide resource capacity mechanism</skos:prefLabel>
    <skos:definition xml:lang="en">This is a market-wide resource capacity mechanism where all resource capacity required to ensure security of supply receives payment. These mechanisms are open for participation to all capacity resources contributing to adequacy.</skos:definition>
  </skos:Concept>
