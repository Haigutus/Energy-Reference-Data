    "@skos:prefLabel": "Market wide resource capacity mechanism",
    "@skos:definition": {
        "@language": "en",
        "@value": "This is a market-wide resource capacity mechanism where all resource capacity required to ensure security of supply receives payment. These mechanisms are open for participation to all capacity resources contributing to adequacy."
