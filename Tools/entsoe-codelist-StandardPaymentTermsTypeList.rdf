    <skos:prefLabel>StandardPaymentTermsTypeList</skos:prefLabel>
    <dc:identifier>ET0041</dc:identifier>
    <skos:definition xml:lang="en">The identification of the different terms of payment.</skos:definition>
  </skos:ConceptScheme>
  <skos:Concept rdf:about="http://data.europa.eu/energy/StandardPaymentTermsTypeList/A01">
    <skos:inScheme rdf:resource="http://data.europa.eu/energy/StandardPaymentTermsTypeList"/>
    <skos:topConceptOf rdf:resource="http://data.europa.eu/energy/StandardPaymentTermsTypeList"/>
    <dc:identifier>A01</dc:identifier>
    <skos:prefLabel>Pay as bid</skos:prefLabel>
    <skos:definition xml:lang="en">The amount to be paid shall correspond to the amount bid.</skos:definition>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.europa.eu/energy/StandardPaymentTermsTypeList/A02">
    <skos:inScheme rdf:resource="http://data.europa.eu/energy/StandardPaymentTermsTypeList"/>
    <skos:topConceptOf rdf:resource="http://data.europa.eu/energy/StandardPaymentTermsTypeList"/>
    <dc:identifier>A02</dc:identifier>
    <skos:prefLabel>Pay as cleared</skos:prefLabel>
    <skos:definition xml:lang="en">The amount to be paid shall correspond to the amount calculated for clearing.</skos:definition>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.europa.eu/energy/StandardPaymentTermsTypeList/A03">
    <skos:inScheme rdf:resource="http://data.europa.eu/energy/StandardPaymentTermsTypeList"/>
    <skos:topConceptOf rdf:resource="http://data.europa.eu/energy/StandardPaymentTermsTypeList"/>
    <dc:identifier>A03</dc:identifier>
    <skos:prefLabel>No payment terms</skos:prefLabel>
    <skos:definition xml:lang="en">There are no payment terms to be used.</skos:definition>
  </skos:Concept>
