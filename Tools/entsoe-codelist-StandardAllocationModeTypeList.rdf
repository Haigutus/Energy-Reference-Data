  <skos:ConceptScheme rdf:about="http://data.europa.eu/energy/StandardAllocationModeTypeList">
    <dcterms:modified>2021-12-03</dcterms:modified>
    <dcat:version>80</dcat:version>
    <skos:prefLabel>StandardAllocationModeTypeList</skos:prefLabel>
    <dc:identifier>ET0040</dc:identifier>
    <skos:definition xml:lang="en">The identification of the method of allocation in an auction.</skos:definition>
  </skos:ConceptScheme>
  <skos:Concept rdf:about="http://data.europa.eu/energy/StandardAllocationModeTypeList/A01">
    <skos:inScheme rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <skos:topConceptOf rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <dc:identifier>A01</dc:identifier>
    <skos:prefLabel>Order by price with pro rata</skos:prefLabel>
    <skos:definition xml:lang="en">The allocation method is by price with eventual pro rata.</skos:definition>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.europa.eu/energy/StandardAllocationModeTypeList/A02">
    <skos:inScheme rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <skos:topConceptOf rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <dc:identifier>A02</dc:identifier>
    <skos:prefLabel>Order by price with first come - first served</skos:prefLabel>
    <skos:definition xml:lang="en">The allocation method is by price with eventual use of first come first served.</skos:definition>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.europa.eu/energy/StandardAllocationModeTypeList/A03">
    <skos:inScheme rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <skos:topConceptOf rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <dc:identifier>A03</dc:identifier>
    <skos:prefLabel>First come - First served</skos:prefLabel>
    <skos:definition xml:lang="en">The allocation method is first come first served.</skos:definition>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.europa.eu/energy/StandardAllocationModeTypeList/A04">
    <skos:inScheme rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <skos:topConceptOf rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <dc:identifier>A04</dc:identifier>
    <skos:prefLabel>Pro rata</skos:prefLabel>
    <skos:definition xml:lang="en">The allocation method is pro rata.</skos:definition>
  </skos:Concept>
  <skos:Concept rdf:about="http://data.europa.eu/energy/StandardAllocationModeTypeList/A05">
    <skos:inScheme rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <skos:topConceptOf rdf:resource="http://data.europa.eu/energy/StandardAllocationModeTypeList"/>
    <dc:identifier>A05</dc:identifier>
    <skos:prefLabel>Continuous</skos:prefLabel>
    <skos:definition xml:lang="en">The allocation method is continuous, i.e. there is no gate closure time when bids from the market participants are collected. Instead allocation procedure takes place immediately.</skos:definition>
  </skos:Concept>
